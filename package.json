{
  "name": "@ktsstudio/mediaproject-utils",
  "version": "6.0.0",
  "author": "KTS Studio",
  "license": "MIT",
  "description": "Package with basic utils for media projects",
  "keywords": [
    "vk-mini-apps",
    "axios",
    "utils"
  ],
  "main": "./dist/cjs/index.js",
  "browser": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src --fix",
    "codestyle": "prettier --write \"src/**/*.{ts,js}?(x)\"",
    "tsc-check": "tsc --skipLibCheck --noEmit --project tsconfig.json",
    "precommit": "yarn run tsc-check && lint-staged",
    "test": "jest",
    "test:cover": "jest --coverage --collectCoverageFrom='src/**/*.{ts,tsx}'"
  },
  "lint-staged": {
    "*.{ts,js}?(x)": [
      "yarn eslint --quiet",
      "yarn prettier --check",
      "yarn jest --bail --findRelatedTests"
    ]
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "logo.png"
  ],
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@swc/core": "^1.3.49",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.4.5",
    "@types/react": ">=17",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.14.1",
    "axios": "^1.6.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^7.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.5.1",
    "react": ">=17",
    "react-dom": "^17.0.0 || ^18.0.0",
    "rollup": "^4.9.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "axios": "^0.24.0",
    "react": ">=17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktsstudio/front-utils.git"
  },
  "private": false,
  "bugs": {
    "url": "https://github.com/ktsstudio/front-utils/issues"
  },
  "homepage": "https://github.com/ktsstudio/front-utils#readme"
}
