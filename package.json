{
  "name": "@ktsstudio/mediaproject-utils",
  "version": "3.0.0",
  "author": "KTS Studio",
  "license": "MIT",
  "description": "Package with basic utils for media projects",
  "keywords": [
    "vk-mini-apps",
    "axios",
    "utils"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint src --fix",
    "codestyle": "prettier src --write \"*.{ts,js}?(x)\"",
    "tsc-check": "tsc --skipLibCheck --noEmit --project tsconfig.json",
    "build": "tsc --module es6 --target es6 --outDir dist --declarationDir dist/types",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist --declarationDir dist/types",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js}?(x)": [
      "yarn run lint",
      "yarn run codestyle"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.4.5",
    "@types/react": "~17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "peerDependencies": {
    "react": "~17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktsstudio/front-utils.git"
  },
  "private": false,
  "bugs": {
    "url": "https://github.com/ktsstudio/front-utils/issues"
  },
  "homepage": "https://github.com/ktsstudio/front-utils#readme"
}
